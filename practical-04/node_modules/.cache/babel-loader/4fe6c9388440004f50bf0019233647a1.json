{"ast":null,"code":"// import EditModalView from \"./EditModalView.vue\";\nexport default {\n  name: 'GalleryCard',\n  // components: {\n  //   EditModalView\n  // },\n  props: {\n    id: String,\n    carName: String,\n    carImage: String,\n    carDetails: String,\n    carPrice: Number\n  },\n\n  data() {\n    return {\n      carData: [],\n      isModalVisible: false\n    };\n  },\n\n  methods: {\n    dataDelete(id) {\n      this.$parent.deleteData(id);\n    },\n\n    clickInfo() {\n      alert(`car price is ₹ ${this.carPrice}`);\n    },\n\n    showModal() {\n      this.isModalVisible = true;\n    },\n\n    closeModal() {\n      this.isModalVisible = false;\n    },\n\n    editCarDetails(id) {\n      // console.log(id);\n      const carItem = this.carData.find(carItem => carItem.id === id);\n      this.initialValues = carItem;\n      this.isModalVisible = true;\n      this.$parent.fetchData();\n    },\n\n    getshowModal(values) {\n      this.isModalVisible = values;\n    }\n\n  },\n  computed: {\n    isPrice() {\n      return this.carPrice === undefined;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA0BA;AAEA,eAAe;AACbA,MAAI,EAAE,aADO;AAEb;AACA;AACA;AACAC,OAAK,EAAE;AACLC,MAAE,EAAEC,MADC;AAELC,WAAO,EAAED,MAFJ;AAGLE,YAAQ,EAAEF,MAHL;AAILG,cAAU,EAAEH,MAJP;AAKLI,YAAQ,EAAEC;AALL,GALM;;AAYbC,MAAI,GAAG;AACL,WAAO;AACLC,aAAO,EAAE,EADJ;AAELC,oBAAc,EAAE;AAFX,KAAP;AAID,GAjBY;;AAkBbC,SAAO,EAAE;AACPC,cAAU,CAACX,EAAD,EAAK;AACb,WAAKY,OAAL,CAAaC,UAAb,CAAwBb,EAAxB;AACD,KAHM;;AAIPc,aAAS,GAAG;AACVC,WAAK,CAAE,kBAAiB,KAAKV,QAAS,EAAjC,CAAL;AACD,KANM;;AAOPW,aAAS,GAAG;AACV,WAAKP,cAAL,GAAsB,IAAtB;AAED,KAVM;;AAWPQ,cAAU,GAAG;AACX,WAAKR,cAAL,GAAsB,KAAtB;AACD,KAbM;;AAeTS,kBAAc,CAAClB,EAAD,EAAK;AACf;AACA,YAAMmB,OAAM,GAAI,KAAKX,OAAL,CAAaY,IAAb,CAAmBD,OAAD,IAAaA,OAAO,CAACnB,EAAR,KAAeA,EAA9C,CAAhB;AACA,WAAKqB,aAAL,GAAqBF,OAArB;AACA,WAAKV,cAAL,GAAsB,IAAtB;AACA,WAAKG,OAAL,CAAaU,SAAb;AACD,KArBM;;AAuBPC,gBAAY,CAACC,MAAD,EAAS;AACnB,WAAKf,cAAL,GAAoBe,MAApB;AACF;;AAzBO,GAlBI;AA8CbC,UAAQ,EAAE;AACRC,WAAO,GAAG;AACR,aAAQ,KAAKrB,QAAL,KAAkBsB,SAA1B;AACD;;AAHO;AA9CG,CAAf","names":["name","props","id","String","carName","carImage","carDetails","carPrice","Number","data","carData","isModalVisible","methods","dataDelete","$parent","deleteData","clickInfo","alert","showModal","closeModal","editCarDetails","carItem","find","initialValues","fetchData","getshowModal","values","computed","isPrice","undefined"],"sourceRoot":"","sources":["/home/mansinakrani/Documents/vuejs-practicals/practical-04/src/components/GalleryCard.vue"],"sourcesContent":["<template>\n  <div>\n    <div>\n      <div class=\"edit-delete-btn\">\n          <p type=\"button\" class=\"btn-edit\" @click=\"showModal\" ><i class=\"fa fa-pencil\" @click=\"editCarDetails(id)\" ></i></p>\n          <p type=\"button\" class=\"btn-delete\" @click=\"dataDelete(id)\">x</p>  \n        </div>\n      <div class=\"carName\">{{ carName }}</div>\n      <div v-if=\"carImage\"><img :src=\"carImage\" alt=\"car-image\" /></div>\n      <div class=\"carDetails\">{{ carDetails }}</div>\n      <button :disabled=\"isPrice\" class=\"btn btn-info text-black\" @click=\"clickInfo\">\n        <span v-if=\"carPrice\">Info</span>\n        <span v-else>Available soon...</span>\n      </button>\n    </div>\n      <!-- <div class=\"edit-form\">\n        <EditModalView v-show=\"isModalVisible\"\n          @close=\"closeModal\" \n          @checkEditCarModalFlag=\"getshowModal\"\n          :initialValues=\"initialValues\"\n        />      \n  </div> -->\n  </div>\n</template>\n\n<script>\n// import EditModalView from \"./EditModalView.vue\";\n\nexport default {\n  name: 'GalleryCard',\n  // components: {\n  //   EditModalView\n  // },\n  props: {\n    id: String,\n    carName: String,\n    carImage: String,\n    carDetails: String,\n    carPrice: Number,\n  },\n  data() {\n    return {\n      carData: [],\n      isModalVisible: false,\n    };\n  },\n  methods: {\n    dataDelete(id) {\n      this.$parent.deleteData(id);\n    },\n    clickInfo() {\n      alert(`car price is ₹ ${this.carPrice}`);\n    },\n    showModal() {\n      this.isModalVisible = true;\n      \n    },\n    closeModal() {     \n      this.isModalVisible = false;\n    },\n   \n  editCarDetails(id) {  \n      // console.log(id);\n      const carItem = this.carData.find((carItem) => carItem.id === id);\n      this.initialValues = carItem;\n      this.isModalVisible = true;\n      this.$parent.fetchData();\n    },\n\n    getshowModal(values) {\n      this.isModalVisible=values;\n    }\n  },\n     \n  computed: {\n    isPrice() {\n      return (this.carPrice === undefined)\n    },\n  },\n};\n</script>\n\n<style>\n.edit-delete-btn {\n  display: flex;\n  justify-content: flex-end;\n  margin-bottom: -40px;\n}\n\n.btn-edit {\n  float: right;\n  border: none;\n  font-size: 20px;\n  margin-right: 10px;\n  cursor: pointer;\n  font-weight: bold;\n  color: #0b577a;\n  background: transparent;\n}\n\n.btn-delete {\n  float: right;\n  border: none;\n  font-size: 20px;\n  margin-right: 10px;\n  cursor: pointer;\n  font-weight: bold;\n  color: #ef3925;\n  background: transparent;\n}\n\n.btn-delete:hover {\n  color: #dd7468;\n}\n\n.carName {\n  margin: 20px 0 0;\n  font-style: oblique;\n  font-size: 20px;\n  text-shadow: 5px 5px 5px rgb(103, 126, 128);\n}\n\nimg {\n  height: 200px;\n  width: 280px;\n}\n\n.carDetails {\n  font-size: medium;\n  height: 160px;\n}\n\n.btn:active,\n.btn:hover {\n  outline: 0;\n  background-color: #f9fafb!important;\n  border-color: #87cbd9!important;\n} \n\n.btn-info.disabled, .btn-info:disabled {\n  color: rgb(81, 77, 77)!important;\n  background-color: #f9fafb!important;\n  border-color: rgb(81, 77, 77)!important;\n}\n \n@media (min-width: 768px) {\n  .btn {\n    font-size: 14px;\n    min-width: 106px;\n  }\n}\n\n@media (max-width: 300px) {\n  img {\n    height: 100px;\n    width: 150px;\n  }\n}\n\n@media (max-width: 700px) {\n  img {\n    height: 150px;\n    width: 250px;\n  }\n\n  .carName {\n    margin: 20px 0 0;\n    font-style: oblique;\n    font-size: 20px;\n  }\n\n  .carDetails {\n    font-size: small;\n    height: 100px;\n  }\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}